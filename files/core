		{
			ste::tester::setzero();
			tester*p=get<tester>[50]();
			stest_accert(tester::getbuildtime()==50);
			get_resize(p,60);
			//destroytime:50(+50) cause move(move_construct&destruct).
			stest_accert(tester::getdestroytime()==50);
			stest_accert(tester::getbuildtime()==60);
