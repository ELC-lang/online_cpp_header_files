namespace array_n{
	//BLOCK:for debug
	inline void test(){
		ELC_TEST_EVENTNAME("array部分测试");
		{
			array_t<int>anarray;
			stest_accert(anarray.size()==0);
			anarray.resize(5);
			stest_accert(anarray.size()==5);
			anarray[2]=99;
			stest_accert(anarray[2]==99);
			anarray.resize(3);
			stest_accert(anarray[2]==99);
			anarray=anarray;
			swap(anarray,anarray);
		}
		check_memory_lack();
	}
	inline void test_log_out(){
	}
	inline void test_end(){
	}
	//BLOCK_END
